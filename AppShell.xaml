<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="CodexGigas.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:local="clr-namespace:CodexGigas"
	xmlns:views="clr-namespace:CodexGigas.Views">

	<TabBar>
        <ShellContent
			Title="Ecuaciones No Lineales No Polinomiales"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:EcuacionesNoLinealesNoPolinomialesPage}" />
        <ShellContent
			Title="Ecuaciones Polinomiales"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:EcuacionesPolinomialesPage}" />
        <ShellContent
			Title="Interpolacion Polinomial, Polinomios Ortogonales, Ajuste Curvas"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:InterpolacionPolinomialPolinomiosOrtogonalesAjusteCurvasPage}" />
        <ShellContent
			Title="Integracion Numerica"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:IntegracionNumericaPage}" />
        <ShellContent
			Title="Ecuaciones Diferenciales Ordinarias Parciales"
			Icon="iconblank.png"
			ContentTemplate="{DataTemplate views:EcuacionesDiferencialesOrdinariasParcialesPage}" />
	</TabBar>
</Shell>
